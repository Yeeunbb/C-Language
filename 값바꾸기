#include <iostream>
using namespace std;
void swap (int *px, int *py);

int main()
{

	int a =100, b=200;
	cout <<"swap() 호출 전: a=" <<a<< ",b=" <<b<<endl;
	swap(&a,&b);
	cout <<"swap() 호출 후: a=" <<a<< ",b=" <<b<<endl;
	return 0;
}

void swap(int *px, int *py)
{
	int temp;
	temp= *px;
	*px=*py;
	*py=temp;
}

